{
  "name": "task1",
  "version": "1.0.0",
  "description": "Event Queue implementation with Jest tests",
  "main": "eventQueue.js",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "devDependencies": {
    "jest": "^29.7.0"
  },
  "jest": {
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "*.js",
      "!jest.config.js"
    ],
    "coverageReporters": [
      "text",
      "html"
    ],
    "verbose": true
  }
}
